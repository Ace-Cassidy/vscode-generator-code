{
	"name": <%- JSON.stringify(name) %>,
	"description": <%- JSON.stringify(description) %>,
	"version": "0.0.1",
	"publisher": <%- JSON.stringify(publisher) %>,
	"engines": {
		"vscode": <%- JSON.stringify(vsCodeEngine) %>
	},
	"categories": [
		"Others"
	],
	"activationEvents": [
		"onCommand:extension.sayHello"
	],
	"main": "./extension",
	"contributes": {
		"commands": [{
			"command": "extension.sayHello",
			"title": "Hello World"
		}]
	},
	"devDependencies": {
		"vscode": "next"
	}
}